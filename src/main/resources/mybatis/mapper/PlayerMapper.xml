<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.house.cjh_cashflow.dao.PlayerDao">

    <insert id="insertPlayer">
        insert into player(name,career,room_code) values(#{name}, #{career}, #{roomCode})
    </insert>


    <select id="selectId" resultType="java.lang.Long">
        select id from player where name = #{name} and room_code = #{roomCode}
    </select>

</mapper>